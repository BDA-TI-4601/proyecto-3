<div class="page-header">
  <%= link_to logins_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_login_path(@login), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show login</h1>
</div>

<dl class="dl-horizontal">
  <dt>Username:</dt>
  <dd><%= @login.username %></dd>

  <dt>Password:</dt>
  <dd><%= @login.password %></dd>

  <dt>Id:</dt>
  <dd><%= @login.id %></dd>

  <dt>Telephone:</dt>
  <dd><%= @login.telephone %></dd>

  <dt>Wishes:</dt>
  <dd><%= @login.wishes %></dd>

  <dt>Reservations:</dt>
  <dd><%= @login.reservations %></dd>

</dl>
